/dts-v1/;
/plugin/;

/ {
        compatible = "rockchip,rk3399";

        fragment@0 {
                target = <&dsi>;
                __overlay__ {
                        status = "okay";
                };
        };

        fragment@1 {
                target = <&gt9xx>;
                __overlay__ {
                        status = "okay";
                };
        };

        fragment@2 {
                target = <&cdn_dp>;
                __overlay__ {
                        status = "disabled";
                };
        };

        fragment@3 {
                target = <&hdmi>;
                __overlay__ {
                        status = "disabled";
                };
        };
};
